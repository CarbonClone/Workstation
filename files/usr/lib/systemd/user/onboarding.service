[Unit]
Description=Run onboarding service
After=network.service
After=network-online.target
ConditionPathExists=!%h/.config/onboarding/disable
#Requires=graphical-session.target

[Service]
#TimeoutStartSec=30
Restart=on-failure
ExecStartPre=sleep 10s
ExecStart=/var/home/eddie/Documents/GitHub/Workstation/files/usr/bin/onboarding


[Install]
WantedBy=default.target
